{
  "version": 2,
  "builds": [
  {
    "src": "frontend/public", 
    "use": "@vercel/static"
  },
  {
    "src": "backend/server.js", 
    "use": "@vercel/node"
  }
],

 "rewrites": [
  {
    "source": "/api/(.*)", 
    "destination": "backend/server.js"
  },
  {
    "source": "/**", 
    "destination": "frontend/src/index.html"
  }
]


}

